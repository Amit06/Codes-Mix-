def partition(a,l,r):
    small=l;
    
    for j in range(l+1,r):
        if a[j]<a[l]:
            small+=1
            a[small],a[j]=a[j],a[small]
    a[l],a[small]=a[small],a[l]
    
    return small

def qs(a,l,h):
    if l<h:
        pos=partition(a, l, h+1)
        qs(a,l,pos-1)
        qs(a,pos+1,h)
        
for i in range(input()):    
    n,k=map(int, raw_input().split())
    a=map(int, raw_input().split())
    qs(a,0,len(a)-1)
    print a[-k:][::-1]