r,c=map(int,raw_input().split())
p=[]
for i in range(r):
    p.append([i for i in raw_input()])
x=[]
for i in range(r):
    for j in range(c):
        if p[i][j]=='*':
            x.append([i,j])    
r,c=[],[]
for i in x:
    for j in x:
        if i[0]==j[0]:
            r.append([i,j])
        if i[1]==j[1]:
            c.append([i,j])

print r,c