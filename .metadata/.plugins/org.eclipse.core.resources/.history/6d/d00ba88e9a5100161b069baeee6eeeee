def adj(x):
    ans=[]
    xn=[i for i in x]
    for p,i in enumerate(xn):
        if i=='a':
            ans.append([xn[:p]+['b']+xn[p+1]]):
        ans.append()

from _collections import deque

start=raw_input()
end=raw_input()
