def countInv(a):
    if len(a)>1:
        print a
        q=len(a)//2
        L=a[:q]
        R=a[q:]
        x=countInv(L)
        y=countInv(R)
        z,l,r=countSplitInv(L,R)
        a=l+r
        print a
        print x,y,z
        return x+y+z
    else:
        return 0
        
def countSplitInv(L,R):
    print "CSI--",L,R
    i=0
    j=0
    
    count=0
    while i<len(L) and j<len(R):
        if L[i]<=R[j]:
            i+=1
        else:
            print len(L)-(i)
            count+=len(L)-(i)
            L[i],R[j]=R[j],L[i]
            j+=1
            
    print L,R             
    return (count,L,R)

