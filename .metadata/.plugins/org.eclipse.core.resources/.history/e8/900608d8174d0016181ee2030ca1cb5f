dp=[]
n=input()
dp.append([0]+[i for i in range(1,n+1)])
dp.append([0 for i in range(n+1)])
dp.append([0 for i in range(n+1)])
dp.append([0 for i in range(n+1)])

print dp
coins=[1,5,7,10]
for j in range(1,4):
    for i in range(1,n+1):
        #print dp[j-1][i],dp[j][i-coins[j]]
        dp[j][i]=min(dp[j-1][i],1+dp[j][i-coins[j]]) if i>coins[j] else dp[j-1][i]

print dp