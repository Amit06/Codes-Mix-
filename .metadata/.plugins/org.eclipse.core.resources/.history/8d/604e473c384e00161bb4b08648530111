def partition(a,l,r):
    small=l;
    
    for j in range(l+1,r):
        if a[j]<a[l]:
            small+=1
            a[small],a[j]=a[j],a[small]
    a[l],a[small]=a[small],a[l]
    
    return small

def qs(as,l,h):
    if l<h-1:
        pos=partition(a, l, h)
        qs(a,l,pos-1)
        qs(a,pos+1,r)
        
for i in range(input()):    
    n,k=map(int, raw_input().split())
    a=map(int, raw_input().split())
    qs(a,0,len(a))