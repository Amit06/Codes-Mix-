r,c=map(int,raw_input().split())
p=[]
for i in range(r):
    p.append([i for i in raw_input()])
x=[]
for i in range(r):
    for j in range(c):
        if p[i][j]=='*':
            x.append([i,j])    
if not x:
    print "YES"
    print "1 1"
else:
    flag=False
    cb=x[0]
    r2=x[0][0]
    c2=x[1][1]
    for i in range(r):
        for j in range(c):
            if i!=r2 and j!=c2:
                continue
            eb=[(u,v) for u in range(r) for v in range(c) if (u==i or v==j) and p[i][j]=='*']
            print eb
            if eb==x:
                print "YES"
                print i,j
                flag=True
                break
    
    if not flag:
        print "NO"