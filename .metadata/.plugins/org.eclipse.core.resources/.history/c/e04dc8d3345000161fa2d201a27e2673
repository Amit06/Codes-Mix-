n,m=map(int,raw_input().split())
x=[True for _ in range(n)]
y=[True for _ in range(n)]
z=[]
for i in range(m):
    p,q=map(int, raw_input().split())
    x[p-1]=False
    y[q-1]=False
    z=[[u,v] for u in range(n) for v in range(n) if x[u]==True and y[v]==True]
    print x
    print y
    print z
    print len(z)