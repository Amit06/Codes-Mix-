for i in range(input()):
    n=input()
    a=map(int,raw_input().split())
    mid=len(a)//2
    start=0
    end=len(a)
    for i in range(10):
        print a[mid],a[mid-1],a[mid+1]
        if a[mid]==mid+1:
            
            if a[mid+1]-a[mid]==2:
                print a[mid]+1
                break
            else:
                mid+=(len(a)-mid)//2
        elif a[mid]==mid+2:
            
            if a[mid]-a[mid-1]==2:
                print a[mid]-1
                break
            else:
                mid-=mid//2
        