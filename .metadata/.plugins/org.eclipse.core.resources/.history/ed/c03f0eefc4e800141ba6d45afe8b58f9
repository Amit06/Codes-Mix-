
public class Test {

	public static void main(String[] args) {
		Stack s=new Stack();
		LStack l=new LStack();
		try{
			s.pop();
			l.pop();
			}
		catch(Exception e)
		{
			System.out.println(e.getMessage());
		}
		s.push(2);
		l.push(5);
		s.push(6);
		l.push(1);
		l.push(9);
		s.push(6);
		s.display();
		l.display();
		try{
			System.out.println(s.pop());
			System.out.println("LS "+l.pop());
				
			}
		catch(Exception e)
		{
			System.out.println(e.getMessage());
		}
		s.display();
		l.display();
	}
}
