r,c=map(int,raw_input().split())
p=[]
for i in range(r):
    p.append([i for i in raw_input()])

x=[]
for i in range(r):
    for j in range(c):
        if p[i][j]=='*':
            x.append([i,j])   
if not x:
    print "YES"
    print "1 1"
else:
    flag=False
    cb=x[0]
    r2=x[0][0]
    c2=x[0][1]
    for i in range(r):
        for j in range(c):
            print i,j
            if i!=r2 and j!=c2:
                continue
            eb=[[u,j] for u in range(r) if p[u][j]=='*']+[[i,u] for u in range(c) if p[i][u]=='*']
            print x,"\n",eb
            if len(eb)==len(x):
                print "YES"
                print i+1,j+1
                flag=True
                break
        if flag:
            break    
    if not flag:
        print "NO"