def partition(a,l,r):
    print a
    small=l;
    
    for j in range(l+1,r):
        if a[j]<a[l]:
            small+=1
            a[small],a[j]=a[j],a[small]
    a[0],a[small]=a[small],a[0]
    print a
    return small+1
for i in range(input()):    
    n,k=map(int, raw_input().split())
    a=map(int, raw_input().split())
    pos=partition(a)
    while(pos!=k):
        if pos<k:
            partition(a,0,pos)
        else:
            partition(a,pos,len(a))