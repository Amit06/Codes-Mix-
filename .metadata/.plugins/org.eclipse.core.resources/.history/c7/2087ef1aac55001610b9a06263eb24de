def cmpa(x,y):
    if x[0]==y[0]:
        if x[1]==y[1]:
            if x[2]==y[2]:
                if x[3]<=y[3]:
                    return x[3]
                else:
                    return y[3]
            elif x[2]<y[2]:
                return x[2]
            else:
                return y[2]
        elif x[1]<y[1]:
            return x[1]
        else:
            return y[1]
    elif x[0]<y[0]:
        return -1
    else:
        return 1
            
for tc in range(int(raw_input())):
    n=int(raw_input())
    ab=[]
    for i in range(n):
        a=raw_input().split()
        ab.append(a)
    ab=sorted(ab,cmp=cmpa)
    print ab