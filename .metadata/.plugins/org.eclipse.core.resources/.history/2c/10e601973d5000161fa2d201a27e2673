n,m=map(int,raw_input().split())
x=[0 for _ in range(n)]
y=[0 for _ in range(n)]
noc,nor=0,0
z=[]
count=n**2
i=0
for col in range(m):
    p,q=map(int, raw_input().split())
    #print p,q
    if x[q-1]==0 and y[p-1]==0:
        count-=2*n-1
        count+=noc+nor
        noc+=1
        nor+=1
        x[p-1]+=1
        y[q-1]+=1
        print x,y
    elif x[p-1]>0 and y[q-1]:
        continue:
    elif x[p-1]>0:
        print "Here 1",i,n
        count-=n-1
        #print "c",count
        count+=(nor-1)
        #print "c",count
        #print "c",count
        noc+=1
        x[p-1]+=1
    elif y[q-1]>0:
        print "Here 2",i,n
        count-=n-1
        #print "c",count
        count+=(noc-1)
        #print "c",count
        #print "c",count
        nor+=1
        y[q-1]+=1
    print x,y,nor,noc
    print count