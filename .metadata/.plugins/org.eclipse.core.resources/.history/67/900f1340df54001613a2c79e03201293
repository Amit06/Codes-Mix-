class Anokhi():
    def __init__(self,sc,ti,sub,nam):
        self.sc=sc
        self.ti=ti
        self.sub=sub
        self.nam=nam
    def __le__(self,x):
        if self.sc<x.sc:
            return -1
        if self.ti<x.ti:
            return -1
        if self.sub<x.sub:
            return -1
        if self.nam<x.nam:
            return -1
for tc in range(input(raw_input())):
    n=int(raw_input())
    ab=[]
    for i in range(n):
        a=raw_input().split()
        temp=Anokhi(int(a[0]),int(a[1]),int(a[2]),a[3])
        ab.append(temp)
    ab=sorted(ab)
    for k in ab:
        print k.sc,k.ti,k.sub,k.nam
    