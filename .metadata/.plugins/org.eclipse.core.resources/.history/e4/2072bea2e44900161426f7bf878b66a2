t,s,x=map(int,raw_input().split())
flag=True
if t<=x:
    for i in range(t,x+1,s):
        print i,
        if i>t and (x==i or x==i+1):
            print 'YES'
            flag=False
            break
        elif i==t and x==i:
            print 'YES'
            flag=False
            break
else:
    print "NO"
if flag==True:
    print "NO"