n,m=map(int,raw_input().split())
x=[True for _ in range(n)]
y=[True for _ in range(n)]
z=[]
count=n**2
i=0
for i in range(m):
    p,q=map(int, raw_input().split())
    if x[p-1]==True and y[q-1]==True:
        count-=2*n-1
        count+=i
        i+=2
        x[p-1]=False
        y[q-1]=False
    elif x[p-1]==False:
        print "Here"
        count-=2*n-1
        count+=i//2
        count+=n-1
        y[q-1]=False
        i+=1
    elif y[q-1]==False:
        count-=2*n-1
        count+=i//2
        count+=n-1
        x[p-1]=False
        i+=1
    print x,y
    print count