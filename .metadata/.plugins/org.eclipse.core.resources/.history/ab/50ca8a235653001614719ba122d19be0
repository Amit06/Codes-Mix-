for i in range(input()):
    n=input()
    a=map(int,raw_input().split())
    mid=len(a)//2
    while True:
        if a[mid]==mid+1:
            if (len(a)-mid-1)//2==0:
                print mid,a[mid]
                break
            else:
                mid+=(len(a)-mid)//2
        elif a[mid]==mid+2:
            mid/=2