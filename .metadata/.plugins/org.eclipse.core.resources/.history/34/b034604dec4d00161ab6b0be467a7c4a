r,c=map(int,raw_input().split())
p=[]
for i in range(r):
    p.append([i for i in raw_input()])
X,Y=[],[]
for i in p:
    ic=0
    for j in i:
        if j=='*':
            ic+=1
    X.append(ic)

for i in range(c):
    ic=0
    for j in range(r):
        if p[j][i]=='*':
            ic+=1
    Y.append(ic)
print X,Y

x=[]
ctr=0
for i in range(r):
    for j in range(c):
        if p[i][j]=='*':
            ctr+=1
            
if ctr==0:
    print "YES"
    print "1 1"

    