for tc in range(input()):
    f,b,t,fd,bd=map(int,raw_input().split())
    if f==b and f<fd:
        print "No Ditch"
    elif f==b and f>=fd:
        print "%d F" %(fd*t)
    elif f!=b and f>=fd:
        print "%d F" %(fd*t)
    elif f!=b and f<fd :
        flag=False
        if f>b:
            m='F'
            
        else:
            flag=True
            m='B'
            fd,bd=bd,fd
            f,b=b,f
        ti=0
        p=0
        if flag:
            ti-=b
            p+=b*t
            
        while(True):
            
            ti+=f
            
            
            if ti>=fd:
                
                p+=(f-(ti-fd))*t
                
                
                break
            else:
                
                p+=f*t
                
                
            ti-=b
            
            p+=b*t
            
        print "%d %s" %(p,m)