class Anokhi():
    def __init__(self,sc,ti,sub,nam):
        self.sc=sc
        self.ti=ti
        self.sub=sub
        self.nam=nam
    def __le__(self,x):
        if x.sc>self.sc:
            return self.sc
        if x.ti>self.ti:
            return self.ti
        if x.sub>self.sub:
            return x.sub
        if len(x.nam)>len(self.nam):
            return self.nam
        
for _ in range(input()):
    n=input()
    ab=[]
    for i in range(n):
        a=raw_input().split()
        temp=Anokhi(int(a[0]),int(a[1]),int(a[2]),a[3])
        ab.append(temp)
    ab=sorted(ab)
    for k in ab:
        print k.sc,k.ti,k.sub,k.nam
    