class anokhi():
    def __init__(self,sc,ti,sub,nam):
        self.sc=sc
        self.ti=ti
        self.sub=sub
        self.nam=nam
    def __le__(self,x):
        if x.sc>self.sc:
            return self.sc
        if x.ti>self.ti:
            return self.ti
        if x.sun>self.sub:
            return self.sub
        
for _ in range(input()):
    n=input()
    a=[]
    for i in range(n):
        a=raw_input().split()
        temp=anokhi(int(a[0]),int(a[1]),int(a[2]),a[3])
        print temp
    