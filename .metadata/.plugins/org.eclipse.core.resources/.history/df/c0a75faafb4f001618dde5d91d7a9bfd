for tc in range(input()):
    f,b,t,fd,bd=map(int,raw_input().split())
    if f==b and f<fd:
        print "No Ditch"
    elif f==b and f>=fd:
        print "%d F" %(fd*t)
    elif f!=b and f>=fd:
        print "%d F" %(fd*t)
    elif f!=b and f<fd :
        if f>b:
            m='F'
            
        else:
            m='B'
            fd,bd=bd,fd
        ti=0
        p=0
        
        while(True):
            
            ti+=f
            
            print "ti--",ti
            if ti>=fd:
                print "p1--",
                p+=(f-(ti-fd))*t
                print p
                break
            else:
                print "p2--",
                p+=f*t
                print p
                
            ti-=b
            print "ti2--",ti
            p+=b*t
            print "p--",p
        print "%d %s" %(p,m)