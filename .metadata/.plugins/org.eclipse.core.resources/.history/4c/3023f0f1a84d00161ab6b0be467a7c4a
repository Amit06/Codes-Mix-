def partition(a):
    small=0;
    
    for j in range(1,len(a)):
        if a[j]<a[0]:
            small+=1
            a[small],a[j]=a[j],a[small]
    a[0],a[small]=a[small],a[0]
    return small
for i in range(input()):    
    n,k=map(int, raw_input().split())
    a=map(int, raw_input().split())
    pos=partition(a)
    while(pos!=k):
        if pos < k:
            partition(a[pos+1:])
        elif pos > k:
            partition(a[:pos])
    print a[k+1:]