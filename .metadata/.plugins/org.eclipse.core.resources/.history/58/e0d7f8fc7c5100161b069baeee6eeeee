n=input()
r,c=map(int,raw_input().split())
m=map(int,raw_input().split())
mat=[]
for i in range(0,r,c):
    print "i--",i
    mat.append(m[i:c])
    
for t in mat:
    for y in t:
        print y,
    print
    
