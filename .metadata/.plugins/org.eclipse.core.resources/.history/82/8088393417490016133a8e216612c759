def countInv(a):
    if len(a)>1:
        print a
        q=len(a)//2
        L=a[:q]
        R=a[q:]
        x=countInv(L)
        y=countInv(R)
        z=countSplitInv(a,L,R)
        return x+y+z
    else:
        return 0
        
def countSplitInv(a,L,R):
    print L,R
    i=0
    j=0
    k=0
    count=0
    while i<len(L) and j<len(R):
        if L[i]<=R[j]:
            a[k]=L[i]
            i+=1
        else:
            a[k]=R[j]
            j+=1
            count+=len(L)-i
        k+=1
    return count

n=open("D:\Integer.txt",'r');

a=[]
for line in n:
    a.append(int(line))

a=[3,2,1]
print countInv(a)
        
        
